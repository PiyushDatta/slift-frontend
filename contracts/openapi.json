  "components": {
    "schemas": {
      "AuthStartResponse": {
        "type": "object",
        "required": ["auth_id", "provider", "auth_url", "status"],
        "properties": {
          "auth_id": {
            "type": "string"
          },
          "provider": {
            "type": "string",
            "enum": ["google", "twitter"]
          },
          "auth_url": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": ["pending"]
          }
        },
        "additionalProperties": false
      },
      "AuthUser": {
        "type": "object",
        "required": ["id"],
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "handle": {
            "type": "string"
          },
          "avatar_url": {
            "type": "string"
          },
          "provider": {
            "type": "string",
            "enum": ["google", "twitter"]
          }
        },
        "additionalProperties": false
      },
      "AuthAckResponse": {
        "type": "object",
        "required": ["auth_id", "status", "acknowledged"],
        "properties": {
          "auth_id": {
            "type": "string"
          },
          "provider": {
            "type": "string",
            "enum": ["google", "twitter"]
          },
          "status": {
            "type": "string",
            "enum": ["pending", "success", "error"]
          },
          "acknowledged": {
            "type": "boolean"
          },
          "error": {
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/AuthUser"
          },
          "session_token": {
            "type": "string"
